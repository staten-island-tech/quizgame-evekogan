html,
body {
  box-sizing: border-box;
  font-family: "Poppins", sans-serif;
}

body {
  font-size: 62.5%;
  padding: 0;
  margin: 0;
  font-family: Stardew_Valley;
  background-color: #f6bb6a;
}

.selection-text {
  color: #672714;
  text-shadow: -1px 0px #ffc891;
}

@font-face {
  font-family: Stardew_Valley;
  src: url("91cebd4cc24332801cb3e39727cdfc05");
}

.selection-box {
  background-color: #dc7b06;
  padding: 1rem;
  height: auto;
  border: 2px solid #693802;
}